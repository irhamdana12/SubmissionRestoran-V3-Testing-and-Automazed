{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent":"off",
		"eol-last":"off",
        "object-curly-newline": "off",
		"import/no-extraneous-dependencies":0,
        "linebreak-style":0,
        "no-tabs":0,
        "operator-linebreak":0,
        "no-underscore-dangle":0,
        "space-before-function-paren":0,
        "no-console":0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "import/prefer-default-export": 0,
        "no-return-assign": 0,
        "class-methods-use-this": 0,
        "no-new": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "no-alert": 0
    }
}
